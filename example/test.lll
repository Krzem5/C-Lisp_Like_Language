(-> (= i 1) (< i 11) {
	(-> (= j 1) (< j 11) {
		(:> i '*' j " is equal to " (* i j) "\n")
		(= j (+ j 1))
	})
	(= i (+ i 1))
})
(= func (,,, x y {
	(:> "(Function Args: {x, y} = {" x ", " y "}) ")
	(@@ (* x y))
}))
(= FACTORIAL 10)
(= i 1 (:> "After Var Init (i=" i ")\n"))
(-> (= j 2) (< j FACTORIAL) {
	(? (< j 5) {
		(:> "Small: " i "\n")
	}
	(< j 8) {
		(:> "Medium: " i "\n")
	} {
		(:> "End: " i " (j=" j ")\n")
	})
	(= i (* i j))
	(? (< j 5) (:> j "! = " i "\n"))
	(= j (+ j 1))
	(? (> 3 10 -5) (:> "This will Never Execute\n"))
})
